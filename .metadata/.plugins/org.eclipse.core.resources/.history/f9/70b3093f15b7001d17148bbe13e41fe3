<script type="text/javascript">
	str = "웬디 70 80 90";
	arr = str.split(" ");
	for(i=0;i<arr.length;i++) {
		document.write(arr[i] + " ");
	}
	document.write("<br>");
	
	
	sum = 0;
	for(i=1;i<arr.length;i++) {
		sum += +arr[i];
	}
	document.write( "sum : " + sum + " ");
	document.write("<br>");
	
</script>
</head>
